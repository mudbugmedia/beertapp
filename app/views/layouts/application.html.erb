<!DOCTYPE html>
<html>
<head>
  <title>Beertap</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <script type="text/javascript" src="//use.typekit.net/ytd8xnd.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
</head>
<body>
	<div class="wrapper">
		<header class="lg-all md-all sm-all clearfix">
			<a href="/"><img src="/images/logo.png" alt="Beer Tap" width="100px" height="100px"></a>
			<div class="header-links lg4-lg8">
				<a href="#"><span data-icon="&#xe617;" aria-hidden="true"></span> My Friends</a>
				<a href="#"><span data-icon="&#xe616;" aria-hidden="true"></span> My Journal</a>
				<a href="#"><span data-icon="&#xe619;" aria-hidden="true"></span> Sign Out</a>
			</div>
		</header>
	</div>



<div class="wrapper clearfix">
  <div class="clearfix"></div>
	<%= yield %>
</div>

<div class="clearfix"></div>
<footer class="clearfix lg-all md-all sm-all">
</footer>
<div class="clearfix"></div>
<div class="clearfix"></div><div class="clearfix"></div><div class="clearfix"></div><div class="clearfix"></div>
<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
<!-- Place this before your </body> tag. -->
<script type="text/javascript">
  var rumbleBannerPlacement = "bottom";
</script>
<script src="http://s0.railsrumble.com/banner.js" type="text/javascript"></script>
<%= javascript_include_tag "gridset/gridset-overlay", "data-turbolinks-track" => true %>
</body>
</html>
